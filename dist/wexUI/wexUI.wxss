/*
 * 功能：表单组件-按钮
 *
 * 描述：使用小程序自带的按钮
 * 创建日期：2017-11-27
 * 更新日期：2017-11-27
 */

.btn {
    display: inline-block;
    padding-left: 32rpx;
    padding-right: 32rpx;
    height: 72rpx;
    line-height: 72rpx;
    text-align: center;
    font-size: 28rpx;
    outline: none;
    white-space: nowrap;
    text-decoration: none;
    box-sizing: border-box;
    border-width: 2rpx;
    border-radius: 4rpx;
    font-weight: 400;
    color: #4b4c50;
    transition: color 0.2s linear, background-color 0.2s linear, border 0.2s linear;
    -webkit-transition: color 0.2s linear, background-color 0.2s linear, border 0.2s linear;
}

/*去除自带button按钮的边框*/

.btn::after {
    content: "";
    box-sizing: border-box;
    border-color: transparent;
    border-radius: 4rpx;
}

/*default-按钮*/

.btn.btn-default {
    background-color: #fff;
    border: solid 2rpx #dddee1;
}

.btn.btn-default:active {
    color: #2d8cf0;
    border-color: #57a3f3;
}

/*dashed-按钮*/

.btn.btn-dashed {
    background: #fff;
    border-style: dashed;
    border-color: #dddee1;
}

.btn.btn-dashed:active {
    color: #2d8cf0;
    border-color: #2d8cf0;
}

/*dashed-按钮*/

.btn.btn-primary {
    color: #fff;
    border-color: #2d8cf0;
    background-color: #2d8cf0;
}

.btn.btn-primary:active {
    border-color: #57a3f3;
    background-color: #57a3f3;
}

/*success-按钮*/

.btn.btn-success {
    color: #fff;
    background: #29b327;
    border-color: #29b327;
}

.btn.btn-success:active {
    background-color: #5bc558;
    border-color: #5bc558;
}

/*info-按钮*/

.btn.btn-info {
    color: #fff;
    background: #30b8f7;
    border-color: #30b8f7;
}

.btn.btn-info:active {
    background-color: #59c7f9;
    border-color: #59c7f9;
    text-decoration: none;
}

/*warn-按钮*/

.btn.btn-warn {
    color: #fff;
    background: #ff870d;
    border-color: #ff870d;
}

.btn.btn-warn:active {
    background-color: #f9a650;
