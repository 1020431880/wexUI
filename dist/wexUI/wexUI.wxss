.wexui-accordion .accordion-list .accordion-content {
    height: 0px;
    overflow: hidden;
    position: relative;
}

.wexui-accordion .accordion-list .accordion-content .accordion-item {
    opacity: 1;
    -webkit-transform: translateY(-50%);
