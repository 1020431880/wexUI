/*
 * 功能：页面展示-弹出层
 *
 * 描述：弹出层
 * 创建日期：2018-2-12
 * 更新日期：2018-2-12
 */

.wexui-popup {
    position: relative;
    visibility: hidden;
}

/*遮罩层样式*/

.wexui-popup .popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 1000;
    background: rgba(0, 0, 0, 0.7);
    opacity: 0;
    transition: all .3s;
}

.popup-show {
    visibility: visible;
}

.popup-show .popup-mask {
    opacity: 1;
}

/*中间content样式*/

.wexui-popup .popup-container {
    position: fixed;
    top: auto;
    left: auto;
    right: auto;
    bottom: auto;
    transform-origin: center;
    box-sizing: border-box;
    background: #fff;
    transition: all .3s;
    z-index: 1001;
}

/*显示内容-中间显示*/

.wexui-popup.popup-center .popup-container {
    top: 50%;
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
    margin: 0 auto;
    border-radius: 4rpx;
    opacity: 0;
}

.wexui-popup.popup-show.popup-center .popup-container {
    opacity: 1;
}

/*显示内容-顶部显示*/

.wexui-popup.popup-top .popup-container {
    top: 0;
    left: 0;
    right: 0;
    transform: translate3d(0, -100%, 0);
}

.wexui-popup.popup-show.popup-top .popup-container {
    transform: translate3d(0, 0, 0);
}

/*显示内容-底部显示*/

.wexui-popup.popup-bottom .popup-container {
    left: 0;
    right: 0;
    bottom: 0;
    transform: translate3d(0, 100%, 0);
}

.wexui-popup.popup-show.popup-bottom .popup-container {
    transform: translate3d(0, 0, 0);
}

/*显示内容-左边显示*/

.wexui-popup.popup-left .popup-container {
    left: 0;
    bottom: 0;
    top: 0;
    transform: translate3d(-100%, 0, 0);
}

.wexui-popup.popup-show.popup-left .popup-container {
    transform: translate3d(0, 0, 0);
}

/*显示内容-右边显示*/

.wexui-popup.popup-right .popup-container {
    right: 0;
    bottom: 0;
    top: 0;
    transform: translate3d(100%, 0, 0);
}

.wexui-popup.popup-show.popup-right .popup-container {
    transform: translate3d(0, 0, 0);
}

